package com.ibm.training.bean;

import java.sql.Date;

public class Event {
	int id;
	String title;
	Date date;
	
	public Event(){
		
	}
	
	public Event(int id, String title, Date date) {
		this.id = id;
		this.title = title;
		this.date = date;
	}

	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	
	
}
